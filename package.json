{
  "name": "six-cities",
  "version": "4.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.rest.js",
    "start:dev": "npm run ts ./src/main.rest.ts | npx pino-pretty --colorize --translateTime SYS:standard",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "tsx",
    "mock:server": "json-server ./src/mocks/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.6",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/got": "^9.6.12",
    "@types/inversify": "^2.0.33",
    "@types/mime-types": "^3.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.19.130",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "json-server": "^1.0.0-beta.3",
    "nodemon": "^3.1.11",
    "pino-pretty": "^13.1.2",
    "rimraf": "3.0.2",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.6",
    "typescript": "^4.9.5"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "@typegoose/typegoose": "^12.20.0",
    "chalk": "^5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-async-handler": "^1.2.0",
    "got": "^12.6.1",
    "http-status-codes": "^2.3.0",
    "inversify": "^7.10.4",
    "mime-types": "^3.0.2",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2"
  }
}
